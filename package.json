{
  "name": "react-date-range",
  "version": "1.0.0-beta",
  "description": "A React component for choosing dates and date ranges.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "NODE_ENV=development & webpack-dev-server --open --mode development --hot --inline",
    "clear": "rm -rf dist/* & rm -rf demo/dist/*",
    "build": "NODE_ENV=production & yarn build-library & yarn build-demo",
    "build-demo": "webpack -p",
    "build-library": "babel ./src --out-dir ./dist --ignore test.js & postcss 'src/styles.scss' -d dist --ext css & postcss 'src/theme/*.scss' -d 'dist/theme' --ext css",
    "lint": "eslint 'src/**/*.js'",
    "test": "jest",
    "preversion": "yarn clear & yarn build"
  },
  "keywords": [
    "react",
    "date",
    "range",
    "datepicker",
    "rangepicker"
  ],
  "contributors": [
    "Burak Can <neoberg@gmail.com> (https://github.com/burakcan)",
    "Mehmet Kamil Morcay <mehmetmorcay@gmail.com> (https://github.com/mkg0)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/Adphorus/react-date-range"
  },
  "bugs": {
    "url": "http://github.com/Adphorus/react-date-range/issues"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "classnames": "^2.2.1",
    "date-fns": "2.0.0-alpha.7",
    "prop-types": "^15.5.10",
    "react-list": "^0.8.8"
  },
  "peerDependencies": {
    "react": "^0.14 || ^15.0.0-rc || >=15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.2.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-date-fns": "^0.2.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "normalize.css": "^8.0.0",
    "postcss": "^7.0.5",
    "postcss-cli": "^6.0.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "precss": "^3.1.2",
    "prettier": "^1.14.3",
    "react-hot-loader": "^4.3.11",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-hot-middleware": "^2.24.3"
  }
}
